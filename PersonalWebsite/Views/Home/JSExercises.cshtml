<a href="" data-toggle="modal" data-target="#max">Max of Five</a>

<div class="modal fade" id="max" tabindex="-1" role="dialog"
     aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header body-blue">
                <button type="button" class=" close" data-dismiss="modal"
                        aria-hidden="true">x</button>
                <h1 class="modal-title" id="myModalLabel">Max of Five</h1>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-sm-6">
                    <p>This will find the highest value based on the user input.</p>
                        <div class="row">
                            <div class="col-sm-6">
                                <p>Enter 5 numbers:</p>
                                <form>
                                     <div class="row">
                                        <div class="col-sm-2">
                                           <input type="number" id="num1" class="form-control" />
                                        </div>
                                        <div class="col-sm-2">
                                            <input type="number" id="num2" class="form-control" />
                                        </div>
                                        <div class="col-sm-2">
                                            <input type="number" id="num3" class="form-control" />
                                        </div>
                                        <div class="col-sm-2">
                                            <input type="number" id="num4" class="form-control" />
                                        </div>
                                        <div class="col-sm-2">
                                            <input type="number" id="num5" class="form-control" />
                                        </div>
                                        <div class="col-sm-2">
                                            <button type="button" id="doMax" class="btn btn-primary">Try it out!</button>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-6">
                                            <div id="highest"> </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                <div class="col-sm-6">
                    <pre class="brush: js text">
function maxOfFive()
{
	var max = numberList[0];
	for(i = 0; i < numberList.length; i++){
		if (numberList[i+1] > max){
			max = numberList[i+1];
		}
	}
	document.querySelector('#MaxNum').innerHTML = "The Max is: " + max;
} 
                </pre>
                </div>
            </div>
            <div class="modal-footer no-margin">
            </div>
        </div>
    </div>
</div>
</div>

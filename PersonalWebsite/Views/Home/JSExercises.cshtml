            sum5 = Number(sum5);
            if (sum1+sum2+sum3+sum4+sum5 == "") {
                document.getElementById("summation").innerHTML = "RESULT: Please fill out at least one of the number spaces.";
            }
            else {
                output = sum1 + sum2 + sum3 + sum4 + sum5;
                document.getElementById("summation").innerHTML = "RESULT: " + output;
            }
        }
        function findProduct() {
            var output = 0;
            var num1 = document.getElementById("multi1").value;
            num1 = Number(num1);
            var num2 = document.getElementById("multi2").value;
            num2 = Number(num2);

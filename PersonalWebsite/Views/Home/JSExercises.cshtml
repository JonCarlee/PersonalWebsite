        if (num1+num2+num3+num4+num5 == "") {
            document.getElementById("sum").innerHTML = "Please fill out at least one number space.";
        }
        else {
            document.getElementById("sum").innerHTML = "RESULT: " + num1+num2+num3+num4+num5;
        }
    }
</script>


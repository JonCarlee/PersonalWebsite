            num5 = Number(num5);
            if (num1 * num2 * num3 * num4 * num5 == "") {
                document.getElementById("product").innerHTML = "RESULT: Please fill out all of the number spaces.";
            }
            else {
                output = num1 * num2 * num3 * num4 * num5;
                document.getElementById("product").innerHTML = "RESULT: " + output;
            }
        }
        function findFactorial() {
            var num = document.getElementById("fact").value;
            num = Math.floor(num);
            var total = 0;
            if (num < 1 || num == "") {
                document.getElementById("factTotal").innerHTML = "RESULT: Please enter a positive integer.";
            }
            else if (num == 1) {
                document.getElementById("factTotal").innerHTML = "RESULT: 1";
            }
            else {
                total = num
                for (i = num - 1; i > 1; i--) {
                    total = total * i;
                }
                document.getElementById("factTotal").innerHTML = "RESULT: " + total;
            }
        }
    </script>

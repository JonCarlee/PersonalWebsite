        function findMax() {
            var num1 = document.getElementById("num1").value;
            var num2 = document.getElementById("num2").value;
            var num3 = document.getElementById("num3").value;
            var num4 = document.getElementById("num4").value;
            var num5 = document.getElementById("num5").value;
            if (num1*num2*num3*num4*num5 == "") {
                document.getElementById("highest").innerHTML = "RESULT: Please fill out all of the number spaces.";
            }
            else {
                document.getElementById("highest").innerHTML = "RESULT: " + Math.max(num1, num2, num3, num4, num5);
            }
        }
        function findSum() {
            var output = 0;
            var sum1 = document.getElementById("sum1").value;
            sum1 = Number(sum1);
